{
	"version":1,
	"formatters":{
		"fmt":{
			"format":"[%(asctime)s][%(thread)d][%(levelname)s][%(filename)s][%(lineno)d] %(message)s",
			"datefmt":"%Y-%m-%d %H:%M:%S"
		}
	},
	"handlers":{
		"consoleHandler":{
			"class":"logging.StreamHandler",
			"formatter":"fmt",
			"level":"DEBUG",
			"stream":"ext://sys.stdout"
		},
		"ExchangeServerfileHandler":{
			"class":"logging.handlers.RotatingFileHandler",
			"formatter":"fmt",
			"level":"DEBUG",
			"filename":"../Log/ExchangeServer.log",
			"mode":"a",
			"maxBytes":52428800,
			"backupCount":20,
			"encoding":"utf-8"
		},
		"ClientfileHandler":{
			"class":"logging.handlers.RotatingFileHandler",
			"formatter":"fmt",
			"level":"DEBUG",
			"filename":"../Log/Client.log",
			"mode":"a",
			"maxBytes":52428800,
			"backupCount":20,
			"encoding":"utf-8"
		},
		"GuifileHandler":{
			"class":"logging.handlers.RotatingFileHandler",
			"formatter":"fmt",
			"level":"DEBUG",
			"filename":"../Log/Gui.log",
			"mode":"a",
			"maxBytes":52428800,
			"backupCount":20,
			"encoding":"utf-8"
		}
	},
	"loggers":{
		"ExchangeServerLogger":{
			"level":"DEBUG",
			"handlers":["consoleHandler","ExchangeServerfileHandler"]
		},
		"ClientLogger":{
			"level":"DEBUG",
			"handlers":["consoleHandler","ClientfileHandler"]
		},
		"GuiLogger":{
			"level":"DEBUG",
			"handlers":["consoleHandler","GuifileHandler"]
		}

	}		
}